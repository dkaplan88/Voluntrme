<div class="event"> 
<div id="search">	<form action="<%= events_url %>" method="get">
    <label for="keyword">Search For An Opportunity Of A Lifetime:</label>
    <input type="search" name="keyword" placeholder="Life Changer Box" size="30">
  </form></div>
	
	<div class = "table_body">
	  <table>
	    <tr>
	      <th>Name</th>
	      <th>Organization</th>
	      <th>Location</th>
	      <th>Category</th>
	    </tr>

	  <% @events.each do |event| %>
	    <tr>
	      <td><%= link_to event.name, event %></td>
	      <td><%= event.organization.name %></td>
	      <td><%= event.location %></td>
	      <td><%= event.category %></td>
	    </tr>
	  <% end %>
	  </table>
	</div>
	
</div>